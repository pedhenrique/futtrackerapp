<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.futtrackerapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_nova2"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="br.com.futtrackerapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ExibeListaTimes"></activity>
        <activity android:name=".activity.ExibeListaJogadores"></activity>
        <activity android:name=".activity.ExibeInfoJogador"></activity>
        <activity android:name=".activity.ExibeVideo"></activity>
        <activity android:name=".activity.TelaInicial"></activity>
    </application>

</manifest>
